<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.srisonkadariya.smartparking">

    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoadingOnlineGate"></activity>
        <activity android:name=".LoadingScreenRequestHistory" />
        <activity android:name=".ReceivedReservationHistory" />
        <activity android:name=".RequestHistory" />
        <activity android:name=".SpotSuccess" />
        <activity android:name=".GateVerification" />
        <activity android:name=".LoadingScreenAvailability" />
        <activity android:name=".ReservationCompletion" />
        <activity android:name=".LoadingScreenFinal" />
        <activity android:name=".History" />
        <activity android:name=".LoadingScreen" />
        <activity android:name=".ResultSpot" />
        <activity android:name=".Recharge" />
        <activity android:name=".login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".UserSession" />
        <activity android:name=".Reservation" />

        <service android:name="org.eclipse.paho.android.service.MqttService" />
    </application>

</manifest>